{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "core:window:allow-drag-window",
    "fs:allow-read-file",
    "fs:allow-exists",
    "fs:allow-stats"
  ]
}
